{
    "name": "jschreuder/middle-skeleton",
    "description": "A Middle framework setup for a new project.",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Middle\\Skeleton\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "jschreuder/middle": "^0.10",
        "zendframework/zend-diactoros": "^1.3",
        "monolog/monolog": "^1.22",
        "robmorgan/phinx": "^0.8",
        "pimple/pimple": "^3.0",
        "symfony/routing": "^3.2",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.2",
        "phpspec/prophecy": "^1.7"
    }
}
