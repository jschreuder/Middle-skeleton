{
    "name": "jschreuder/middle-skeleton",
    "description": "A Middle framework setup for a new project.",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Middle\\Skeleton\\": "src"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-PDO": "*",
        "jschreuder/middle": "dev-master#79ad8f6f823f782db853ac6cf8586fca96122d18",
        "laminas/laminas-diactoros": "^2.6",
        "laminas/laminas-httphandlerrunner": "^2.0",
        "monolog/monolog": "^2.3",
        "robmorgan/phinx": "^0.12",
        "pimple/pimple": "^3.4",
        "symfony/routing": "^5.3",
        "symfony/console": "^5.3"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.1"
    }
}
