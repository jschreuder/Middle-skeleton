{
    "name": "jschreuder/middle-skeleton",
    "description": "A Middle framework setup for a new project.",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Middle\\Skeleton\\": "src"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-PDO": "*",
        "jschreuder/middle": "2.0-RC2",
        "jschreuder/middle-di": "dev-master",
        "laminas/laminas-diactoros": "^2.9",
        "laminas/laminas-httphandlerrunner": "^2.1",
        "monolog/monolog": "^2.3",
        "robmorgan/phinx": "^0.13",
        "symfony/routing": "^6.0",
        "symfony/console": "^6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^7.2"
    }
}
